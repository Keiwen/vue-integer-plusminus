{
  "name": "vue-integer-plusminus",
  "version": "1.5.0",
  "description": "Integer input with increment and decrement buttons for vue 2",
  "author": "Keiwen <keiwen.mk@gmail.com>",
  "private": false,
  "scripts": {
    "dev": "webpack-dev-server --config docs/webpack.config.js --inline --hot",
    "build": "npm run build-src && npm run build-docs",
    "build-src": "webpack --config src/webpack.config.js --progress --hide-modules",
    "build-docs": "webpack --config docs/webpack.config.js --progress --hide-modules"
  },
  "main": "dist/vue-integer-plusminus.js",
  "module": "src/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Keiwen/vue-integer-plusminus.git"
  },
  "dependencies": {
    "vue": "^2.4.4"
  },
  "keywords": [
    "vue",
    "integer",
    "increment",
    "input"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^1.0.6",
    "css-loader": "^0.14.5",
    "file-loader": "^0.8.4",
    "imports-loader": "^0.6.5",
    "node-sass": "^4.13.1",
    "sass-loader": "^6.0.6",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.2",
    "svg-inline-loader": "^0.7.1",
    "unindent": "^2.0.0",
    "vue-highlightjs": "^1.1.0",
    "vue-loader": "^15.9.0",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.0",
    "webpack-merge": "^4.2.2",

    "mixin-deep": ">=1.3.2",
    "set-value": ">=2.0.1",
    "lodash": ">=4.17.19",
    "serialize-javascript": ">=3.1.0",
    "elliptic": ">=6.5.3",
    "websocket-extensions": ">=0.1.4",
    "minimist": ">=0.2.1",
    "kind-of": ">=6.0.3",
    "clean-css": ">=4.1.11"
  }
}
