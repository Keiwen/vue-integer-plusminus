{
  "name": "vue-integer-plusminus",
  "version": "3.0.0",
  "description": "Integer input with increment and decrement buttons for vue 3",
  "author": "Keiwen <keiwen.mk@gmail.com>",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve ./demo/main.js",
    "build:lib": "vue-cli-service build --target lib ./src/index.js",
    "build:docs": "vue-cli-service build ./demo/main.js --mode docs",
    "build": "npm run build:lib && npm run build:docs",
    "lint": "vue-cli-service lint"
  },
  "main": "dist/vue-integer-plusminus.common.js",
  "module": "dist/vue-integer-plusminus.umd.js",
  "browser": "dist/vue-integer-plusminus.umd.min.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Keiwen/vue-integer-plusminus.git"
  },
  "dependencies": {
    "vue": "3.x"
  },
  "keywords": [
    "vue",
    "integer",
    "increment",
    "input"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-standard": "^6.1.0",
    "core-js": "^3.8.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^7.0.0",
    "sass": "^1.83.1",
    "sass-loader": "^16.0.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
